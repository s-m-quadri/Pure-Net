{
  "name": "Pure Net Web Space",
  "description": "This extension will Filter the necessary components of html",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "storage",
    "activeTab",
    "scripting"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/images/get_started16.png",
      "32": "/images/get_started32.png",
      "48": "/images/get_started48.png",
      "128": "/images/get_started128.png"
    }
  },
  "icons": {
    "16": "/images/get_started16.png",
    "32": "/images/get_started32.png",
    "48": "/images/get_started48.png",
    "128": "/images/get_started128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "PureNet_Injection.js"
      ],
      "css":[
        "PureNet_Injection.css"
      ],
      "run_at": "document_end",
      "all_frames": true
    }
  ],
  "options_page": "options.html",
  "web_accessible_resources": [
    {
      "resources": [
        "PureNet_Controler.html",
        "PureNet_Controler.css",
        "PureNet_Controler.js",
        "*"
      ],
      "matches": [
        "*://*/*"
      ],
      "use_dynamic_url": true
    }
  ]
}